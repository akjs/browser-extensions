{
  "manifest_version": 2,
  "name": "AkJS DevTools",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "akjs.png",
    "default_title": "AkJS DevTool"
  },
  "devtools_page": "dev.html"
}
