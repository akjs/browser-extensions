{
  "manifest_version": 2,
  "name": "AkJS Extension Background Script Example",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "akjs.png",

    "default_title": "AkJS Extension 2"
  }
}