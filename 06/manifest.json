{
  "manifest_version": 2,
  "name": "AkJS Popup",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "akjs.png",
    "default_popup": "ui/index.html",
    "default_title": "AkJS"
  },
  "options_page": "ui/options.html",
  "options_ui": {
    "page": "ui/options.html",
    "chrome_style": true
  }
}
